<%- model_class = User -%>
<div class="container users">
	<div class="form-signin">
	<p id="notice"><%= notice %></p>

<dl class="dl-horizontal" style="max-width: 1000px;">
    <dt><strong><%= model_class.human_attribute_name(:first_name) %>:</strong></dt>
    <dd><%= @user.first_name %></dd>
    <dt><strong><%= model_class.human_attribute_name(:last_name) %>:</strong></dt>
    <dd><%= @user.last_name %></dd> 
    <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
    <dd><%= @user.email %></dd> 
  </dl>


	<%= link_to 'Edit', edit_user_registration_path(@user), :class => 'btn btn-primary' %> 
	<%= link_to 'Back', users_path, :class => 'btn btn-default', :style => 'color: black;' %>
</div>

</div>